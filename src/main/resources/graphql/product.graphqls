type Product{
 id:ID!
 productName:String!,
 description:String!,
 createDate:String!,
 price:Float!

}

input ProductInput{
    productName:String!,
    description:String!,
    price:Float!
}

type Query{
    getProducts:[Product]!
    getProductById(id:ID):Product
}

type Mutation{
    saveProduct(product:ProductInput):Product
}
