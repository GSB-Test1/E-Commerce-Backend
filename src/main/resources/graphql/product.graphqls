type Product{
    id:ID!
    productName:String!,
    description:String!,
    createDate:String!,
    price:Float!,
    category:Category
}
type Category{
    id:ID!,
    categoryName:String

}

input ProductInput{
    productName:String!,
    description:String!,
    price:Float!
}

type Query{
    getProducts:[Product]!
    getProductById(id:ID):Product
}

type Mutation{
    saveProduct(product:ProductInput):Product
}
